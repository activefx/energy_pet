$("#day").text("<%= @simulation.day %>");
$("#month_status_code").text("<%= @simulation.last_month_status %>");
$("#neighbor_status_code").text("<%= @simulation.neighbor_status %>");
$("#baseline_status_code").text("<%= @simulation.baseline_status %>");
$("#date h3").text("<%= Time.parse(@simulation.date).strftime("%A, %B %d") %>")

$("#month_goal_percentage").text("<%= ((((1 - @simulation.last_month_percentage) * 100) / @user.target_goal) * 100).to_i %>%");
$("#neighbor_goal_percentage").text("<%= ((((1 - @simulation.neighbor_percentage) * 100) / @user.neighbor_goal) * 100).to_i %>%");
$("#baseline_goal_percentage").text("<%= ((((1 - @simulation.baseline_percentage) * 100) / @user.baseline_goal) * 100).to_i %>%");

